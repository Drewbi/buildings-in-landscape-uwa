{
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest/globals": true
  },
  "extends": ["eslint:recommended", "prettier", "plugin:jest/recommended"],
  "plugins": ["@babel", "prettier", "jest"],
  "rules": {
    "prettier/prettier": "error"
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module",
    "ecmaVersion": 2020
  }
}
